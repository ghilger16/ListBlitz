{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@Components/*": ["app/components/*"]
    }
  },
  "include": ["app/**/*"],
  "extends": "expo/tsconfig.base"
}
