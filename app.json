{
  "expo": {
    "name": "List Blitz",
    "slug": "list-blitz",
    "scheme": "listblitz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/landing-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.listblitz.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.listblitz.app",
      "versionCode": 1
    },
    "plugins": ["expo-router"]
  }
}
