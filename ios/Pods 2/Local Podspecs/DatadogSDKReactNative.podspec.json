{
  "name": "DatadogSDKReactNative",
  "version": "2.8.2",
  "summary": "A client-side React Native module to interact with Datadog",
  "homepage": "https://github.com/DataDog/dd-sdk-reactnative#readme",
  "license": "Apache-2.0",
  "authors": "Datadog (https://github.com/DataDog)",
  "platforms": {
    "ios": "12.0",
    "tvos": "12.0"
  },
  "source": {
    "git": "https://github.com/DataDog/dd-sdk-reactnative.git",
    "tag": "2.8.2"
  },
  "source_files": "ios/Sources/*.{h,m,mm,swift}",
  "dependencies": {
    "React-Core": [],
    "DatadogCore": [
      "~> 2.27.0"
    ],
    "DatadogLogs": [
      "~> 2.27.0"
    ],
    "DatadogTrace": [
      "~> 2.27.0"
    ],
    "DatadogRUM": [
      "~> 2.27.0"
    ],
    "DatadogCrashReporting": [
      "~> 2.27.0"
    ],
    "RCT-Folly": [
      "2024.11.18.00"
    ],
    "glog": [],
    "React-RCTFabric": [],
    "ReactCodegen": [],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "ReactCommon/turbomodule/bridging": [],
    "ReactCommon/turbomodule/core": [],
    "React-NativeModulesApple": [],
    "Yoga": [],
    "React-Fabric": [],
    "React-graphics": [],
    "React-utils": [],
    "React-featureflags": [],
    "React-debug": [],
    "React-ImageManager": [],
    "React-rendererdebug": [],
    "DoubleConversion": [],
    "React-jsi": [],
    "React-renderercss": [],
    "hermes-engine": [],
    "React-hermes": []
  },
  "ios": {
    "dependencies": {
      "DatadogWebViewTracking": [
        "~> 2.27.0"
      ]
    }
  },
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "OTHER_CPLUSPLUSFLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED=1 -DRCT_NEW_ARCH_ENABLED=1 -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -DFOLLY_HAVE_CLOCK_GETTIME=1 -Wno-comma -Wno-shorten-64-to-32",
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/Headers/Private/Yoga\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "OTHER_SWIFT_FLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED"
  },
  "compiler_flags": "-DRCT_NEW_ARCH_ENABLED=1 -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -DFOLLY_HAVE_CLOCK_GETTIME=1 -Wno-comma -Wno-shorten-64-to-32",
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "ios/Tests/**/*.{swift,json}",
      "resources": "ios/Tests/Fixtures",
      "platforms": {
        "ios": "13.4",
        "tvos": "13.4"
      }
    }
  ]
}
